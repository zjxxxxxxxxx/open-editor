{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"]
    },
    "dev:watch": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"],
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "typecheck": {}
  }
}
