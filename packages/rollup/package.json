{
  "name": "@open-editor/rollup",
  "version": "0.0.16",
  "description": "A rollup plugin for fast find source code, supported in react and vue.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm rollup -c --environment __TARGET__:es2020",
    "dev": "pnpm build -w --environment __DEV__",
    "check": "tsc --noEmit",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjxxxxxxxxx/open-editor.git",
    "directory": "packages/rollup"
  },
  "keywords": [
    "rollup-plugin",
    "vue-devtools",
    "react-devtools"
  ],
  "author": "zjxxxxxxxxx <954270063@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zjxxxxxxxxx/open-editor/issues"
  },
  "homepage": "https://github.com/zjxxxxxxxxx/open-editor/tree/main/packages/rollup#readme",
  "dependencies": {
    "@open-editor/client": "workspace:*",
    "@open-editor/server": "workspace:*",
    "@open-editor/shared": "workspace:*"
  },
  "devDependencies": {
    "rollup": "^3.28.1"
  }
}
